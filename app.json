{
  "expo": {
    "name": "MexL Cinema",
    "slug": "mexl-cinema",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/mexl_cinema-1-edit-removebg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mexl_cinema-1-edit-removebg.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mexl_cinema-1-edit-removebg.png",
        "backgroundColor": "#000",
        "package": "com.mexlcinema",
        "versionCode": 1
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.mexlcinema"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9cbf7978-afbc-4050-874f-04e3c8e665b0"
      }
    },
    "owner": "madiajijah11"
  }
}
